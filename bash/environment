export WORKINGDIR=${HOME}/Code
export WORKON_HOME=${HOME}/virtual_envs
export EDITOR=$([ -f /usr/local/bin/nvim ] && echo "$(which nvim)" ||  echo "$(which vi)")
export MYBIN=${HOME}/bin
export GOROOT=/usr/local/go
export PYCHARM_HOME=${MYBIN}/pycharm
export VPNPATH=/opt/cisco/anyconnect/bin
export PATH=${VPNPATH}:${MYBIN}:${PYCHHARM_HOME}/bin:$PATH:${GOROOT}/bin
export GOPATH=${WORKINGDIR}/GO
